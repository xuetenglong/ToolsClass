<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--
        border_thickness         设置边框的宽度
        border_inside_color     设置内边框的颜色
        border_outside_color     设置外边框的颜色
        内外边框如果只设置一个的话，则认为只显示一个边框。
     -->
    <declare-styleable name="RoundImageView">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color" />
    </declare-styleable>

    <declare-styleable name="TasksCompletedView">
        <attr name="radius" format="dimension" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="circleWidth" format="dimension" />
        <attr name="rectWidth" format="dimension" />
        <attr name="circleColor" format="color" />
        <attr name="rectColor" format="color" />
        <attr name="ringColor" format="color" />
    </declare-styleable>


    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer" />
        <attr name="textIsDisplayable" format="boolean" />
        <attr name="style">
            <enum name="STROKE" value="0" />
            <enum name="FILL" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RecordVideoStatueCircle">
        <attr name="statueCircleWidth" format="dimension" />
        <attr name="statueCircleColor" format="color" />
        <attr name="ringWidth" format="dimension" />
        <attr name="ringCol" format="color" />
        <attr name="ringStrokeWidth" format="dimension" />
        <attr name="rectW" format="dimension" />
        <attr name="rectCol" format="color" />
        <attr name="rectRoundW" format="dimension" />
    </declare-styleable>

    <declare-styleable name="StickyScrollView">
        <attr name="stuckShadowHeight" format="dimension" />
        <attr name="stuckShadowDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="ViewfinderView">
        <attr name="firstLineStr" format="string" />
        <attr name="secondLineStr" format="string" />
    </declare-styleable>

    <declare-styleable name="DefaultTimeBar">
        <attr name="buffered_color" format="integer" />
        <attr name="scrubber_color" format="color" />
        <!--<attr name="isWidthMatchParent" format="boolean" />-->
        <!--<attr name="autoRefresh" format="boolean" />-->
        <!--<attr name="autoLoadMore" format="boolean" />-->
    </declare-styleable>

    <declare-styleable name="VideoPlayerErrorView">
        <attr name="error_info_text" format="string|reference" />
        <attr name="error_button_text" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="WavesLayout">
        <attr name="waves_alpha" format="integer" />
        <attr name="waves_alpha_step" format="integer" />
        <attr name="waves_framerate" format="integer" />
        <attr name="waves_duration" format="integer" />
        <attr name="waves_color" format="color" />
        <attr name="waves_animend_alpha" format="integer" />
        <attr name="waves_canshowwave" format="boolean" />
        <attr name="waves_radius" format="dimension" />
        <attr name="waves_radius_top_left" format="dimension" />
        <attr name="waves_radius_top_right" format="dimension" />
        <attr name="waves_radius_bottom_left" format="dimension" />
        <attr name="waves_radius_bottom_right" format="dimension" />
    </declare-styleable>




    <declare-styleable name="RingProgressBar">
        <attr name="back_color" format="color" />
        <attr name="front_color1" format="color" />
        <attr name="front_color2" format="color" />
        <attr name="front_color3" format="color" />
        <attr name="back_width" format="dimension" />
        <attr name="front_width" format="dimension" />
        <attr name="is_need_title" format="boolean" />
        <attr name="is_need_content" format="boolean" />
        <attr name="is_need_unit" format="boolean" />
        <attr name="is_need_dial" format="boolean" />
        <attr name="string_title" format="string" />
        <attr name="string_unit" format="string" />
        <attr name="total_engle" format="integer" />
        <attr name="current_value" format="float" />
        <attr name="max_value" format="float" />
    </declare-styleable>

    <declare-styleable name="FilterView">
        <attr name="is_iconType" format="boolean" />
    </declare-styleable>







    <declare-styleable name="CircleProgressView">
        <!--进度条宽度-->
        <attr name="circle_progress_width" format="dimension" />
        <!--进度条底色-->
        <attr name="circle_progress_bg_color" format="color" />
        <!--进度条开始颜色-->
        <attr name="circle_progress_color_start" format="color" />
        <!--进度条结束颜色-->
        <attr name="circle_progress_color_end" format="color" />
        <!--进度文字大小-->
        <attr name="circle_progress_text_size" format="dimension" />
        <!--进度文字颜色-->
        <attr name="circle_progress_text_color" format="color" />
        <!--进度文字是否粗体-->
        <attr name="circle_progress_text_bold" format="boolean" />
    </declare-styleable>







    <declare-styleable name="WaveView">
        <attr name="above_wave_color" format="color"/>
        <attr name="blow_wave_color" format="color"/>
        <attr name="use_default_alpha" format="boolean"/>
        <attr name="progress" format="integer"/>
        <attr name="wave_length" format="enum">
            <enum name="large" value="1"/>
            <enum name="middle" value="2"/>
            <enum name="little" value="3"/>
        </attr>
        <attr name="wave_style" format="enum">
            <enum name="stop" value="1"/>
            <enum name="move" value="2"/>
        </attr>
        <attr name="wave_height" format="enum">
            <enum name="large" value="1"/>
            <enum name="middle" value="2"/>
            <enum name="little" value="3"/>
        </attr>
        <attr name="wave_hz" format="enum">
            <enum name="fast" value="1"/>
            <enum name="normal" value="2"/>
            <enum name="slow" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="Themes">
        <attr name="waveViewStyle" format="reference"/>
    </declare-styleable>




    <declare-styleable name="CustomExpandableTextView">
        <attr name="maxCollapsedLines" format="integer" />
        <attr name="animDuration" format="integer" />
        <attr name="contentTextSize" format="dimension" />
        <attr name="contentTextColor" format="color" />
        <attr name="contentLineSpacingMultiplier" format="float" />
        <attr name="expandDrawable" format="reference" />
        <attr name="expandText" format="string" />
        <attr name="collapseDrawable" format="reference" />
        <attr name="collapseText" format="string" />
        <attr name="expandCollapseTextSize" format="dimension" />
        <attr name="expandCollapseTextColor" format="color" />
        <attr name="DrawableAndTextGravity">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>


</resources>